  "dependencies": {
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.1",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
